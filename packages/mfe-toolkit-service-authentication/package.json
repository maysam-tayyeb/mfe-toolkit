{
  "name": "@mfe-toolkit/service-authentication",
  "version": "1.0.0",
  "description": "Authentication service for MFE Toolkit",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mfe",
    "microfrontend",
    "auth",
    "authentication",
    "service"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@mfe-toolkit/core": "workspace:*"
  },
  "devDependencies": {
    "@mfe-toolkit/core": "workspace:*",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.6.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js",
      "default": "./dist/types.js"
    }
  }
}
